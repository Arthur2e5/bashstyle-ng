#########################################################

for alias in "$@"; do
	[[ $(grep -w "alias $alias" $HOME/.custom-rc) != "" ]] &&
		{ sed -e "/alias $alias/d" -i $HOME/.custom-rc ; }

	[[ $(alias | grep -w $alias=) != "" ]] &&
		{ unalias $alias ; }
done
