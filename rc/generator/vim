generate_vimrc ()
{

	rc_add \" This config file was generated by BashStyle-NG $BSNG_VERSION
	rc_add \" at $CURRENT_DATE use as \$HOME/.vimrc to apply settings

	. $BSNG_PREFIX/share/bashstyle-ng/rc/settings/vim-rc

}
