generate_readlinerc ()
{

	rc_add \# This config file was generated by BashStyle-NG $BSNG_VERSION
	rc_add \#at $CURRENT_DATE Use as \$HOME/.inputrc to apply settings

	. $BSNG_PREFIX/share/bashstyle-ng/rc/settings/readline-rc

}
