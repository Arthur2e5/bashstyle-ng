if [[ $USE_LSCD == "true" ]]; then

	builtin cd "${@}" &>/dev/null
	$BSNG_RC_DIR/dirinfo/display

	echo -e "${epink}content:"
	ls $LSCD_OPTS
	echo "$PWD" > $HOME/.lastpwd

else	buildin cd "${@}"
fi
